{
    "name": "ianm/level-ranks",
    "description": "Add a level/experience bar to your flarum Forum.",
    "keywords": [
        "extension",
        "flarum",
        "experience",
        "level",
        "xp"
    ],
    "type": "flarum-extension",
    "authors": [
        {
            "name": "Shahiem Seymor",
            "email": "s.seymor@live.nl",
            "homepage": "http://shahiemseymor.com/"
        },
        {
            "name": "IanM",
            "homepage": "https://discuss.flarum.org/u/ianm",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/imorland/level-ranks/issues",
        "source": "https://github.com/imorland/level-ranks"
    },
    "require": {
        "flarum/core": "^1.0.2"
    },
    "extra": {
        "flarum-extension": {
            "title": "Level Ranks",
            "category": "feature",
            "icon": {
                "name": "fas fa-percent",
                "backgroundColor": "#0072e3",
                "color": "#fff"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "IanM\\LevelRanks\\": "src/"
        }
    }
}
